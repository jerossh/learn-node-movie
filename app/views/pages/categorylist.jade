extends ../layout

block content
  .container
    .row
      table.table.table-hover.table-bordered
        thread
          tr
            th 电影分类
            th 录入时间
        tbody
          each item in categories
            tr(class="item-id-#{item._id}")
              td #{item.name}
              td #{moment(item.meta.createdAt).format('MM/DD/YY')}
